#!/usr/bin/env python3
import sys

def celsius(n) :
    return (n-32) * 5/9

def fahrenheit(n) :
    return n*9/5 + 32

def metre(n) :
    return n*1.0936

def yard(n) :
    return n/1.0936

def help() :
    prnt("Referez vous au manuel pour l'utilisation du programme")

def main() :
    print(sys.arv)

if(__name__ == "__main__") :
    main()
